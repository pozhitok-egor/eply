{
  "plugins": [
    ["module-resolver", {
      "extensions": [".ios.js", ".android.js", ".ios.jsx", ".android.jsx", ".js", ".jsx", ".json", ".ts", ".tsx"],
      "root": ["."],
      "alias": {
        "@api": "./src/api",
        "@assets": "./src/assets",
        "@components": "./src/components",
        "@scenes": "./src/scenes",
        "@theme": "./src/app/theme",
        "@utils": "./src/utils",
        "@pages": "./src/pages",
        "@shared": "./src/shared",
        "@ui": "./src/ui",
        "@hooks": "./src/shared/hooks",
        "@helpers": "./src/shared/helpers",
        "@config": "./src/shared/config"
      }
    }]
  ]
}